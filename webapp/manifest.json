{
  "_version": "1.58.0",
  "sap.app": {
    "id": "fiorilibappname",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.3",
      "toolsId": "8721194b-ee4f-4c9f-bd0f-60e02aaf98b1"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZBSK_LA_SOL;v=2/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "fullWidth": true,
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "useBatch": true
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fiorilibappname.i18n.i18n"
        }
      },
      "viewModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {},
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
          "routerClass": "sap.m.routing.Router",
          "viewType": "XML",
          "async": true,
          "viewPath": "fiorilibappname.view",
          "controlId": "app",
          "controlAggregation": "pages",
          "clearControlAggregation": false,
          "sample": {
            "stretch": true,
            "files": [
              "view/Object.view.xml",
              "controller/Object.controller.js",
              "./SharedBlocks/businessDescription/Description.js",
              "./SharedBlocks/businessDescription/Description.view.xml",
              "./SharedBlocks/businessDescription/Image.js",
              "./SharedBlocks/businessDescription/Image.view.xml",
              "./SharedBlocks/technicalInformation/Service.js",
              "./SharedBlocks/technicalInformation/Service.view.xml",
              "./SharedBlocks/technicalInformation/Tar_Map.js",
              "./SharedBlocks/technicalInformation/Tar_Map.view.xml",
              "./SharedBlocks/technicalInformation/Version.js",
              "./SharedBlocks/technicalInformation/Version.view.xml",
              "./SharedBlocks/importantNotes/Comments.js",
              "./SharedBlocks/importantNotes/Comments.view.xml",
              "manifest.json"
            ]
          }
      },
      "routes": [
          {
              "name": "Worklist",
              "pattern": "",
              "target": ["Worklist"]
          },
          {
              "name": "Detail",
              "pattern": "detail/{SolId}",
              "target": ["Object"]
          },
          {
              "name": "Create",
              "pattern": "solution/create",
              "target": ["Object"]
          }
      ],
      "targets": {
          "Worklist": {
              "viewType": "XML",
              "transition": "slide",
              "clearControlAggregation": false,
              "viewId": "Worklist",
              "viewName": "Worklist"
          },
          "Object": {
              "viewType": "XML",
              "transition": "slide",
              "clearControlAggregation": false,
              "viewId": "Object",
              "viewName": "Object"
          },
          "notFound": {
             "id": "notFound",
             "name": "NotFound",
             "transition": "show"
          }
      }
    }
    ,
    "rootView": {
      "viewName": "fiorilibappname.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "resourceRoots": {
      "fiorilibappname.SharedBlocks": "./SharedBlocks"
    }
  }
}
