<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <SelectDialog
        id="tileSelectDialog"
        noDataText="No Tiles Found"
        title="Select Tile"
        search=".onSearchTile"
        multiSelect="true"
        confirm=".onDialogCloseTile"
        cancel=".onDialogCloseTile"
        items="{
            path: 'jsonModelFile>/Tile'
        }">
        <StandardListItem
            title="{jsonModelFile>Title}"
            description="{jsonModelFile>Description}"
            type="Active"/>
    </SelectDialog>
</core:FragmentDefinition>