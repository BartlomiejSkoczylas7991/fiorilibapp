<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <Table items="{singleSolutionModel>/to_Tar_Map}">
        <headerToolbar>
            <Toolbar>
                <Title text="Target Mappings"/>
            </Toolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="TarMapId"/>
            </Column>
            <Column>
                <Text text="Type"/>
            </Column>
            <Column>
                <Text text="Url"/>
            </Column>
            <Column>
                <Text text="ParamValue"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{singleSolutionModel>TarMapId}"/>
                    <Text text="{singleSolutionModel>Type}"/>
                    <Link text="{singleSolutionModel>Url}" href="{singleSolutionModel>Url}" target="_blank"/>
                    <Text text="{singleSolutionModel>ParamValue}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>