<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <Table items="{singleSolutionModel>/Group}">
        <headerToolbar>
            <Toolbar>
            </Toolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="Group ID"/>
            </Column>
            <Column>
                <Text text="Title"/>
            </Column>
            <Column>
                <Text text="Description"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{singleSolutionModel>GroupId}"/>
                    <Text text="{singleSolutionModel>Title}"/>
                    <Text text="{singleSolutionModel>Description}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>
