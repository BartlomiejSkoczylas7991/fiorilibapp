<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <Table items="{singleSolutionModel>/Catalog}">
        <headerToolbar>
            <Toolbar>
                <Title text="Catalog(s)"/>
            </Toolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="Catalog ID"/>
            </Column>
            <Column>
                <Text text="Title"/>
            </Column>
            <Column>
                <Text text="Description"/>
            </Column>
            <Column>
                <Text text="Changed At"/>
            </Column>
            <Column>
                <Text text="Changed By"/>
            </Column>
            <Column>
                <Text text="Created At"/>
            </Column>
            <Column>
                <Text text="Created By"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{singleSolutionModel>CatalogId}"/>
                    <Text text="{singleSolutionModel>Title}"/>
                    <Text text="{singleSolutionModel>Description}"/>
                    <Text text="{singleSolutionModel>CreatedAt}"/>
                    <Text text="{singleSolutionModel>CreatedBy}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>
