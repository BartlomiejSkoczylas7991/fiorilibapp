<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
    <Table items="{singleSolutionModel>/Role}">
        <headerToolbar>
            <Toolbar>
                <Title text="Role(s)"/>
            </Toolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="Role ID"/>
            </Column>
            <Column>
                <Text text="Tech Name"/>
            </Column>
            <Column>
                <Text text="Access Level"/>
            </Column>
            <Column>
                <Text text="Description"/>
            </Column>
            <Column>
                <Text text="Changed At"/>
            </Column>
            <Column>
                <Text text="Changed By"/>
            </Column>
            <Column>
                <Text text="Created At"/>
            </Column>
            <Column>
                <Text text="Created By"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <Text text="{singleSolutionModel>RoleId}"/>
                    <Text text="{singleSolutionModel>TechName}"/>
                    <Text text="{singleSolutionModel>AccessLevel}"/>
                    <Text text="{singleSolutionModel>Description}"/>
                    <Text text="{singleSolutionModel>CreatedAt}"/>
                    <Text text="{singleSolutionModel>CreatedBy}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>
